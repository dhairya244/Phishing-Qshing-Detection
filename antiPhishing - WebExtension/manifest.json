{
  "name": "AntiPhish Guard",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Protects against phishing attacks",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
         
    }
  ]

}

